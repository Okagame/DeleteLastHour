{
  "manifest_version": 3,
  "version": "1.0",
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "permissions": ["browsingData"],
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "default_locale": "en",
  "web_accessible_resources": [
    {
      "resources": ["popup.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "storage",
    "http://*/",
    "https://*/"
  ],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ]
}
